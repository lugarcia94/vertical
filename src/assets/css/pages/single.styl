.single
    
    .sobre__text 
        p 
            a 
                color: #00b7b4

    .tags__post
        width: 100%;
        justify-content: center
        display: flex
        max-height: 22px

        ul 
            li 
                margin-right: 10px
                margin-bottom 0

    .title__single 
        font-size: 36px
        color: #424040
        text-align: center
        letter-spacing: 2px
        max-width: 80%
        margin: 0 auto
        letter-spacing: 0

        @media(max-width 992px)
            max-width 100%

    .coments__date 
        width: 100%
        display: flex
        justify-content: center
        align-items: center
        flex-wrap: wrap
        .data--coments
            max-width 200px

        .author__post 
            color: #777575
            margin-left: 30px

        .share
            margin-left: 30px
.colun__resumo
    margin-top: 50px
    display: flex
    justify-content space-between
    flex-wrap: wrap
    width 100%
    align-items: center

    .colun
        width 260px
        height 260px
        box-shadow: 2.121px 2.121px 14px 0px rgba(0, 0, 0, 0.08)
        padding: 20px

        @media(max-width 992px)
            width 100%

        .colunista__post
            flex-wrap: wrap
        
            .foto__colunita
                flex: 0 0 100%
                text-align: center
                margin-bottom: 20px
            
            .info__colunista
                max-width 100%
                text-align: center

                .nome__colunista, .desc__colunista
                    text-align: center

        .link__colunista
            text-align: center

    .resumo 
        color: #424040
        font-size: 16px
        max-width 550px
        width 100%
        letter-spacing: 0
        text-align: justify

        @media(max-width:992px)
            margin-top: 10px
            text-align: justify

.sobre__text 
    color: #424040
    margin-top: 35px
    font-size: 16px
    letter-spacing: 2px
    margin-bottom: 10px

    @media(max-width:992px)
        text-align: justify

    h1
        font-size: 36px
        text-align: left

textarea#comment 
    display: none

.comentarios__form
    width 100%
    box-shadow: 2.121px 2.121px 14px 0px rgba(0, 0, 0, 0.08)
    padding: 20px
    margin: 20px 0

    h1  
        font-size: 20px
        color: #000
        text-transform: uppercase

    h3#reply-title 
        display: none

    p.comment-notes 
        display: none

    p.coments-p 
        color: #424040
        margin-top: 35px
        font-size: 1rem 
        border-bottom: 1px dotted #00b7b4
        padding-bottom 10px

        &:last-child
            border: none

    .comentario-autor_text 
        width: 100%
        display: block
        margin: 20px 0
        margin-top 5px
        
    .autor-coment 
        color: #03b8b3
        margin-right: 5px

    #author 
        padding-left: 20px
        border: solid 1px #ebebeb
        font-style: normal
        border-radius: 3px
        font-style: italic!important

        &::placeholder
            color: #b4bebe!important
            font-style: italic!important

    textarea#comment1
        border: solid 1px #ebebeb
        padding: 20px
        margin: 10px 0

        &::placeholder
            color: #b4bebe!important
            font-style: italic!important

    p.form-submit 
        text-align: right

        input#submit
            background: #f39e3f
            color: #ffffff
            font-weight: 700
            font-size: 14px
            text-transform: uppercase
            max-width 40%
            margin: 0 auto
            transition: all 150ms ease-in-out
            height 40px
            border: none

            &:hover
                opacity: 0.9

.category
    main#main 
        width: 100%
        max-width: 100%

.autor__footer
    background-image: url('data:image/png;base64,')
    min-height: 200px
    width 100%
    background-size: 100%
    background-repeat: no-repeat 

    @media(max-width: 1199px)
        background-size: cover

    @media(max-width: 768px)
        min-height: 300px


    .autor__block
        display: flex
        align-items: center
        flex-wrap: wrap
        min-height: 142px
        padding: 0 40px 
        justify-content: space-between

        @media(max-width: 1199px)
            justify-content: center

        img 
            max-width 92px
            width auto  

            @media(max-width: 1199px)
                margin-top 20px
            
        .author__post
            max-width 680px

            @media(max-width: 1199px)
                text-align: center

            > span 
                width 100%
                display: block

            .title__autor
                font-size: 13px
                color: #3D3D3D
                font-weight: 600
                letter-spacing: 0.58px
                text-transform: uppercase

            .desc__autor
                font-size: 13px
                color: #f39e3f
            
            .resumo__autor
                font-size: 13px
                color #999999
                letter-spacing: 0.58px
                margin-top 7px